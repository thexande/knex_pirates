	div#vide-island(data-vide-bg="mp4: /video/island.mp4" data-vide-options="postion: 0% 0%, muted: false")  
		.container.text-center
			br
			br
			.row
				.col-md-5.col-md-offset-3
					.card
						img.card-img-top(src='/img/jolly.png', alt='Card image cap')
						.card-block
							h4.card-title 
						ul.list-group.list-group-flush
							li.list-group-item 
								strong Accessory: 
								span 
							li.list-group-item 
								strong Beard: 
								span 
							li.list-group-item 
								strong Drink: 
								span 
						.card-footer.text-muted.text-center
							.btn-group
								a.btn.btn-primary Edit
								a.btn.btn-danger Delete
